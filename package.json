{
  "name": "full-stack-js",
  "private": true,
  "version": "1.0.0",
  "description": "Full stack monorepo (backend, frontend, shared)",
  "packageManager": "pnpm@10.16.1",
  "scripts": {
    "dev": "pnpm -r --parallel --filter @full-stack-js/backend dev & pnpm -r --parallel --filter @full-stack-js/frontend dev",
    "dev:backend": "pnpm --filter @full-stack-js/backend dev",
    "dev:frontend": "pnpm --filter @full-stack-js/frontend dev",
    "build": "pnpm -r build",
    "build:backend": "pnpm --filter @full-stack-js/backend build",
    "build:frontend": "pnpm --filter @full-stack-js/frontend build",
    "lint": "eslint . --ext .ts,.tsx",
    "lint:fix": "eslint . --ext .ts,.tsx --fix",
    "format": "prettier --write ."
  },
  "prettier": {},
  "devDependencies": {
    "@eslint/js": "^9.35.0",
    "@types/cors": "^2.8.19",
    "@types/node": "^20.14.15",
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/swagger-ui-express": "^4.1.8",
    "@typescript-eslint/eslint-plugin": "^8.44.0",
    "@typescript-eslint/parser": "^8.44.0",
    "eslint": "^9.35.0",
    "eslint-import-resolver-typescript": "^4.4.4",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jsx-a11y": "^6.9.0",
    "eslint-plugin-react": "^7.37.2",
    "eslint-plugin-react-hooks": "^5.1.0-rc.0",
    "eslint-plugin-security": "^3.0.1",
    "eslint-plugin-unused-imports": "^4.1.4",
    "prettier": "^3.6.2",
    "typescript": "^5.6.3",
    "typescript-eslint": "^8.8.0"
  }
}
